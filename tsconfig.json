{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "types": ["node"],
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react-jsx"
  },
  "exclude": ["node_modules"],
  "include": ["src"],
}
